<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.saitejapillutla.warehouse">

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.WareHouse"
        android:usesCleartextTraffic="true">
        <activity android:name=".addOrder"></activity>
        <activity android:name=".allModels" />
        <activity
            android:name=".createModel"
            android:noHistory="true" />
        <activity android:name=".itemdetails" />
        <activity android:name=".orderDetails" />
        <activity android:name=".modelDetails" />
        <activity android:name=".qrscan" />
        <activity android:name=".Members" />
        <activity android:name=".ProfileActivity" />
        <activity android:name=".pending" />
        <activity android:name=".HomeActivity" />
        <activity android:name=".createUser" />
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
    </application>

</manifest>